{
  "manifest_version": 3,
  "name": "Tab Managerr",
  "version": "1.0",
  "description": "Manage what happens to your tabs when you launch Chrome. ADD MORE LATER",
  "options_page": "options.html",

  "permissions": ["tabs", "tabGroups", "storage"],

  "background": {
    "service_worker": "background.js"
  },

  "icons": {
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  }
}